{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "app/app": "dev-develop",
        "bartacus/bartacus-bundle": "^3.2",
        "bartacus/platformsh-bundle": "^3.1",
        "bartacus/twig-bundle": "^2.1",
        "clickstorm/cs-seo": "^7.3",
        "helhum/typo3-console": "^7.1",
        "nyholm/psr7": "^1.5",
        "platformsh/config-reader": "^2.4",
        "platformsh/symfonyflex-bridge": "^2.7",
        "symfony/asset": "^5.4",
        "symfony/config": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/routing": "^5.4",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "typo3/cms-backend": "^11.5",
        "typo3/cms-belog": "^11.5",
        "typo3/cms-beuser": "^11.5",
        "typo3/cms-context-help": "^11.5",
        "typo3/cms-core": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3/cms-impexp": "^11.5",
        "typo3/cms-info": "^11.5",
        "typo3/cms-info-pagetsconfig": "^11.5",
        "typo3/cms-lowlevel": "^11.5",
        "typo3/cms-redirects": "^11.5",
        "typo3/cms-reports": "^11.5",
        "typo3/cms-rte-ckeditor": "^11.5",
        "typo3/cms-scheduler": "^11.5",
        "typo3/cms-seo": "^11.5",
        "typo3/cms-setup": "^11.5",
        "typo3/cms-sys-note": "^11.5",
        "typo3/cms-t3editor": "^11.5",
        "typo3/cms-tstemplate": "^11.5",
        "typo3/cms-viewpage": "^11.5",
        "typo3/minimal": "^11.5",
        "willdurand/negotiation": "^3.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4"
    },
    "repositories": [
        {
            "type": "path",
            "url": "public/typo3conf/ext/app",
            "options": {
                "symlink": false
            }
        }
    ],
    "config": {
        "platform": {
            "php": "8.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "cweagans/composer-patches": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "vendor/bin/typo3cms install:fixfolderstructure": "php-script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "enable-patching": true,
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4",
            "docker": false
        }
    }
}
